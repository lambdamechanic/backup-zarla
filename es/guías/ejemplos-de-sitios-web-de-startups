<source
srcset="https://www.zarla.com/images/zarla-ejemplos-de-sitios-web-de-startups-2400x2400-20240520.webp?crop=2:1,smart&width=535 1x, https://www.zarla.com/images/zarla-ejemplos-de-sitios-web-de-startups-2400x2400-20240520.webp?crop=2:1,smart&width=535&dpr=2 2x"
media="(max-width: 575px)"
/>
<source
srcset="https://www.zarla.com/images/zarla-ejemplos-de-sitios-web-de-startups-2400x2400-20240520.webp?crop=2:1,smart&width=727 1x, https://www.zarla.com/images/zarla-ejemplos-de-sitios-web-de-startups-2400x2400-20240520.webp?crop=2:1,smart&width=727&dpr=2 2x"
media="(max-width: 767px)"
/>
<img
class="card-pic"
srcset="https://www.zarla.com/images/zarla-ejemplos-de-sitios-web-de-startups-2400x2400-20240520.webp?crop=2:1,smart&width=730&dpr=2 2x"
