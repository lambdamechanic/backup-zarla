srcset="https://www.zarla.com/images/zarla-catering-web-sitesi-rnekleri-5769x3846-2024063.jpeg?crop=2:1,smart&width=372 1x, https://www.zarla.com/images/zarla-catering-web-sitesi-rnekleri-5769x3846-2024063.jpeg?crop=2:1,smart&width=372&dpr=2 2x"
