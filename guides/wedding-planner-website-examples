<source
data-srcset="https://www.zarla.com/images/zarla-weddings-by-e-20240718-desktop.webp?crop=400:248,offset-x0,offset-y0&width=495&dpr=2" media="(max-width: 575px)" />
<img
class="lazyload"
data-src="https://www.zarla.com/images/zarla-weddings-by-e-20240718-desktop.webp?crop=400:248,offset-x0,offset-y0&width=650&dpr=2"
