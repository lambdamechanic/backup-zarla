<source
data-srcset="https://www.zarla.com/images/zarla-archer-dental-3840x8732-2024071.webp?crop=400:248,offset-x0,offset-y0&width=495&dpr=2" media="(max-width: 575px)" />
<img
class="lazyload"
data-src="https://www.zarla.com/images/zarla-archer-dental-3840x8732-2024071.webp?crop=400:248,offset-x0,offset-y0&width=650&dpr=2"
